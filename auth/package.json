    "argon2": "^0.27.1",
    "bay-common": "0.0.12",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.12.2",
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
